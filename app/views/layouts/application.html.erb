<!DOCTYPE html>
<html>
  <head>
    <title>Cwtchat</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="row">
        <div class="col col-md-10">
          <%= yield %>
        </div>
        <div class="col col-md-2" id="active_users">
          <br>
          <%= render 'layouts/active_users' %>
        </div>
      </div>
    </div>
  </body>
</html>
